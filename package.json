{
  "name": "videotest",
  "version": "1.0.0",
  "dependencies": {
    "expo": "^51.0.37",
    "expo-av": "~14.0.7",
    "expo-build-properties": "~0.12.5",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-video": "^1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "npm:react-native-tvos@0.75-stable"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-tvos/config-tv": "^0.0.11",
    "@types/react": "~18.2.79",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "expo prebuild --clean",
    "prebuild:tv": "EXPO_TV=1 expo prebuild --clean",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  }
}
